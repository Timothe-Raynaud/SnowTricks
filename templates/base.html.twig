<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{{ siteName }}{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        <script>
            window.APP_BASE_PATH = '/public/';
        </script>
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
</body>
</html>
